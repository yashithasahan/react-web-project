{
  "name": "get-your-job",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "autoprefix": "^1.0.1",
    "postcss-cli": "^10.1.0",
    "react": "^18.2.0",
    "react-datepicker": "^4.16.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^2.1.8",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "build:css": "postcss src/index.css -o src/tailwind.css",
    "watch:css": "postcss src/indec.css -o src/tailwind.css -w",
    "start": "npm run build:css && react-scripts --openssl-legacy-provider start",
    "watch": "TAILWIND_MODE='watch' npm start",
    "nodemon": "nodemon start",
    "build": "npm run build:css && react-scripts --openssl-legacy-provider build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "tailwindcss": "^3.3.3"
  }
}
